  "customCSS" : ".error-box{\n    color:red;\n    padding:50px;\n    border:1px solid red;\n    border-radius:4px;\n}\n \n.footer-info{\n    background-color: rgb(93, 134, 46);\n    padding: 15px 0 0 0 !important;\n    height:80px;\n    width:100%;\n}\n\n.footer-content-container{\n\twidth:100%;\n    background-color: #7F8084;\n}\n.footer-content{\n    background-color: #7F8084;\n    color:#FFFFFF;\n    height:65px;\n    padding: 0 10px 0 10px;\n    width:100%;\n    max-width:1440px;\n    margin:auto;\n}\n\n.footer-social{\n\tpadding:18px 12px 12px 0px; \n    float:right;\n}\n.footer-social a{\n\tmargin-left:6px;\n}\n.footer-social a img{\n\twidth:24px;\n    height:24px;\n}\n\n.register-container{\n    padding:50px;\n    border:1px solid #333;\n    border-radius:4px;\n}\n.register-content{\n    text-align:center;\n}\n.register-body,\n.register-header{\n    padding:12px ;\n}\n.register-button{\n    display:block;\n    padding:12px 24px;\n    border-radius:4px;\n    background-color:rgb(93, 134, 46);\n    color:white !important;\n    width:fit-content;\n    margin:12px auto;\n}\n.register-app-header{\n    font-size:16px;\n    color:93, 134, 46;\n    text-align:center;\n    padding:12px 12px 12px 12px;\n    \n}\n.register-app-body{\n    width:fit-content;\n    text-align:center;\n   margin:0 auto;\n}\n.register-app-container {\n    min-height:240px;\n    padding:50px;\n    border:1px solid #333;\n    border-radius:4px;\n}\n.register-app-button{\n    display:block;\n    padding:12px 24px 12px 24px;\n    border-radius:4px;\n    background-color:rgb(93, 134, 46);\n    color:white !important;\n    width:fit-content;\n    margin:12px auto 12px auto;\n}\n.forum-container{\n    padding:40px;\n    border:0px solid #333;\n    border-radius:4px;\n    background-color:rgb(127, 128, 132);\n}\n.forum-content{\n    text-align:center;\n    color:white;\n}\n.forum-body,\n.forum-header{\n    padding:12px\n}\n.forum-button{\n    display:block;\n    padding:12px 24px;\n    border-radius:4px;\n    background-color:rgb(93, 134, 46);\n    color:white !important;\n    width:fit-content;\n    margin:12px auto;\n}\n\n.static-container{\n    background-image:url(../../../sfsites/c/file-asset/manpointing?v=1);\n    height:250px;\n    background-position:center;\n    background-repeat:no-repeat;\n    display:grid;\n    grid-template-columns:50% 50%;\n    border-radius:4px;\n}\n.static-content{\n    text-align:center;\n    grid-column:2;\n    vertical-align: middle;\n    margin: auto;\n}\n.static-body,\n.static-header{\n    padding:12px\n}\n.static-button{\n    display:block;\n    padding:12px 24px;\n    border-radius:4px;\n    background-color:rgb(93, 134, 46);\n    color:white !important;\n    width:fit-content;\n    margin:12px auto;\n}\n\n.breadcrumb-container{\n    margin-top: 4pt;\n    color: #333 !important;\n    font-size: 12px;\n}\n\n.breadcrumb,\n.breadcrumb-last{\n    margin-right: 16pt;\n    color: #333 !important;\n}\n.breadcrumb-last{\n    font-weight:bold;\n}\n.breadcrumb-hr{\n    border:1px solid rgb(93, 134, 46);\n    margin: 16pt 0pt;\n}\n\n.forceChatterScroller{\n    width:100% !important;\n}",
